{
  "expo": {
    "name": "LolTimeFlash",
    "slug": "loltimeflash-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "loltimeflash",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loltimeflash.app",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["mailto"],
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "loltimeflash",
              "com.googleusercontent.apps.463246350287-6rir517otoc45evgs1f8qihciegcccc3"
            ]
          }
        ]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#021022"
      },
      "package": "com.loltimeflash.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/BeaufortforLOL-Regular.otf",
            "./assets/fonts/BeaufortforLOL-Medium.otf",
            "./assets/fonts/BeaufortforLOL-Bold.otf",
            "./assets/fonts/BeaufortforLOL-Heavy.otf"
          ]
        }
      ],
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#021022",
          "image": "./assets/images/splash.png",
          "enableFullScreenImage_legacy": true
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9ed80648-4129-40bf-a651-b4280da556d0"
      }
    },
    "owner": "teczer"
  }
}
