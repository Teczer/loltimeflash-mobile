    "expo-clipboard": "^8.0.8",
    "expo-constants": "18.0.10",
    "expo-dev-client": "6.0.17",
    "expo-font": "14.0.9",
    "expo-glass-effect": "^0.1.8",
    "expo-haptics": "14.0.0",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "8.0.11",
    "expo-localization": "~17.0.8",
