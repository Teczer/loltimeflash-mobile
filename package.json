{
  "name": "loltimeflash-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_USE_FAST_RESOLVER=1 bunx expo start -c",
    "dev": "EXPO_USE_FAST_RESOLVER=1 bunx expo start --dev-client -c",
    "prebuild": "bunx expo prebuild",
    "prebuild:clean": "bunx expo prebuild --clean",
    "android": "bunx expo run:android",
    "android:prebuild": "bun run prebuild && bunx expo run:android",
    "ios": "bunx expo run:ios",
    "ios:prebuild": "bun run prebuild && bunx expo run:ios",
    "web": "bunx expo start --web",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check .",
    "clean": "rm -rf node_modules ios/build android/build ios/Pods ios/Podfile.lock ~/Library/Developer/Xcode/DerivedData/",
    "lint": "eslint .",
    "optimize:champions": "bun run scripts/optimize-champions.ts",
    "sync:items": "bun run scripts/sync-items.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "15.0.3",
